ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/FTPopOverMenu_Swift" "$PODS_CONFIGURATION_BUILD_DIR/MMDrawController" "$PODS_CONFIGURATION_BUILD_DIR/PinCodeTextField" "$PODS_CONFIGURATION_BUILD_DIR/SwiftyNotifications"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/FTPopOverMenu_Swift/FTPopOverMenu_Swift.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MMDrawController/MMDrawController.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/PinCodeTextField/PinCodeTextField.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftyNotifications/SwiftyNotifications.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "FTPopOverMenu_Swift" -framework "MMDrawController" -framework "PinCodeTextField" -framework "SwiftyNotifications"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
